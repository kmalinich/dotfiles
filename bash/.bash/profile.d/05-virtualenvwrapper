# bash profile: virtualenvwrapper
#

# Source virtualenvwrapper
export WORKON_HOME="~/.virtualenvs"

VIRTUALENV_WRAPPER="/usr/bin/virtualenvwrapper.sh"

if [[ ! -s "${VIRTUALENV_WRAPPER}" ]]; then
    return
fi

# Bash completions
source "${VIRTUALENV_WRAPPER}"


